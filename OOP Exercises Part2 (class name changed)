
public class Car {

    private String counrty_of_car;
    private String model;
    private String maker;


    public void setCounrty_of_car ( String counrty ) {
        counrty_of_car = counrty;
    }

    public void setModel ( String s ) {
        model = s;
    }

    public void setMaker ( String n ) {
        maker = n;
    }

    public String getCountry () {
        return counrty_of_car;
    }

    public String getModel () {
        return model;
    }

    public String getMaker () {
        return maker;
    }


