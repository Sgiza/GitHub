public class Main extends Car {


    public static void main ( String[] args ) {

        Car BMW = new Car ( );
        Car Audi = new Car ( );
        Car Audi1 = new Car ( );

        BMW.setCounrty_of_car ( "Germany" );
        BMW.setMaker ( "BMW corporation" );
        BMW.setModel ( "MiniCooper" );

        Audi.setCounrty_of_car ( "Germany" );
        Audi.setMaker ( "Audi Inc" );
        Audi.setModel ( "Audi A3" );

        Audi1.setCounrty_of_car ( "Germany" );
        Audi1.setMaker ( "Audi Inc" );
        Audi1.setModel ( "Audi A6" );

        System.out.println ( BMW.getCountry ( ).toLowerCase () + ' ' + BMW.getModel ( ).toLowerCase () + ' ' + BMW.getMaker ( ).toLowerCase () );
        System.out.println ( Audi.getCountry ( ).toLowerCase () + ' ' + Audi.getModel ( ).toLowerCase () + ' ' + Audi.getMaker ( ).toLowerCase () );
        System.out.println ( Audi1.getCountry ( ).toLowerCase () + ' ' + Audi1.getModel ( ).toLowerCase () + ' ' + Audi1.getMaker ( ).toLowerCase () );


    }
}
